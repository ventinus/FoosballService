{
  "compilerOptions": {
    "alwaysStrict": true,
    "charset": "utf8",
    "declaration": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": [
      "es2016"
    ],
    "module": "CommonJS",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "target": "ES2017",
    "composite": true
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "../assets"
    },
    {
      "path": "../aws-iam"
    },
    {
      "path": "../aws-s3"
    },
    {
      "path": "../core"
    },
    {
      "path": "../cx-api"
    },
    {
      "path": "../assert"
    },
    {
      "path": "../../aws-cdk"
    },
    {
      "path": "../../../tools/cdk-build-tools"
    },
    {
      "path": "../../../tools/cdk-integ-tools"
    },
    {
      "path": "../../../tools/pkglint"
    }
  ],
  "_generated_by_jsii_": "Generated by jsii - safe to delete, and ideally should be in .gitignore"
}
